<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.isen.twinmaxapk">


    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Acquisition"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name=".bleService.activities_frags.BTScanActivity"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".manual.Manual"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name=".bleService.activities_frags.BLE_ScanActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".HistoricActivity"
            android:screenOrientation="landscape">
        </activity>

    </application>

</manifest>
